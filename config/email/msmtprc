defaults
tls on
auto_from off
logfile ~/.msmtp.log

account default
host smtp.gmail.com
tls on
tls_certcheck off
auth on
from razvand@gmail.com
user razvand@gmail.com
passwordeval "cat ~/.authinfo | grep 'razvand@gmail.com' | cut -d ' ' -f 8- | tr -d '\"'"
port 587

